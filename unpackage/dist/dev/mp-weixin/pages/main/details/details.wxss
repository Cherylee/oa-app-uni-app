
.image {
	width:200rpx;
	margin:40rpx 0;
}
.processList-box{
	margin-top: 25rpx;
	padding: 10px 0;
	background-color: #F8F8F8;
}
button {
	margin-top: 30rpx;
	margin-bottom: 30rpx;
}
.button-sp-area {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center
}
.mini-btn {
	/* flex: 1; */
}
/* 提示窗口 */
.uni-tip {

		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;

		padding: 15px;
		width: 300px;
		background-color: #fff;
		border-radius: 10px;
}
.uni-tip-title {
		text-align: center;
		font-weight: bold;
		font-size: 16px;
		color: #333;
}
.uni-tip-content {
		/* padding: 15px;
*/
		font-size: 14px;
		color: #666;
}
.uni-tip-group-button {

		display: -webkit-box;
		display: -webkit-flex;
		display: flex;

		-webkit-box-orient: horizontal;
		-webkit-box-direction: normal;
		-webkit-flex-direction: row;
		        flex-direction: row;
		margin-top: 20px;
}
.uni-tip-button {
		-webkit-box-flex: 1;
		-webkit-flex: 1;
		        flex: 1;
		text-align: center;
		font-size: 14px;
		color: #3b4144;
		border-radius: 0 0 0 0;
		border: 0px;
		color: #FFFFFF;
}
.uni-tip-button1{
		background-color: #CCCCCC;
}
.uni-tip-button2{
		background-color: #4CA2FF;
}
.title {
		padding: 10rpx 25rpx;
}
.uni-icon-clear,
	.uni-icon-eye {
		color: #999;
}
.uni-input{
		background: #F7F7F7;
}
/* 最后节点操作 */
.lastBox{
		margin-top: 30rpx;
		padding: 20rpx 32rpx;
		/* background: #F7F7F7; */
		margin-bottom: 30rpx;
		border: 1rpx solid #F7F7F7;
}
.la-title{
		color: #666666;
		display: inline-block;
}
.uni-input{
		display: inline-block;
		background-color: #F7F7F7;
}
.inputbox{
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: start;
		-webkit-justify-content: flex-start;
		        justify-content: flex-start;
		-webkit-box-align:center ;
		-webkit-align-items:center ;
		        align-items:center ;
		padding: 15rpx 0;
}

